<!DOCTYPE html>
<html>
  <head>
    <title>EvStationFinderFe</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <%= render "layouts/navbar" %>
    <div class="errors">
      <% flash.each do |type, msg| %>
      <div>
        <%= msg %>
      </div>
      <% end %>
    </div>
    <%= yield %>
  </body>
</html>
